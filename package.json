    "connect-mongo": "^1.3.2",
    "csv-stringify": "^1.0.4",
    "ejs": "^2.5.6",
    "express": "^4.15.2",
    "express-session": "^1.15.2",
    "helmet": "^3.5.0",
    "mailgun-js": "^0.10.1",
    "method-override": "^2.3.8",
    "moment": "^2.18.1",
    "mongoose": "^4.9.4",
    "multer": "^1.3.0",
    "newrelic": "^1.38.2",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^4.0.0",
    "shortid": "^2.2.8",
    "twilio": "^2.11.1"
  },
  "devDependencies": {
    "esdoc": "^0.5.2",
    "esdoc-node": "^1.0.1",
    "snyk": "^1.28.1"

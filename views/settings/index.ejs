<% include ../partials/header %>


<div class="container">
    <div class="row">
        <table class="table table-hover">
            <tr>
                <th>Key</th>
                <th>Value</th>
                <th>Edit</th>
            </tr>
            <% settings.forEach(function(setting){ %>
            <tr>
                <td><%= setting.key %></td>
                <td><%= setting.value %></td>
                <td>
                    <a href="/settings/<%= setting._id %>/edit" class="btn btn-xs btn-warning">Edit</a>
                </td>
            </tr>
            <% }); %>
        </table>
    </div>
</div>

<% include ../partials/footer %>
